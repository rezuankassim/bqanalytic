{
    "name": "rezuankassim/bqanalytic",
    "description": "This is a package to ease integration between BigQuery and Firebase Analytics",
    "license": "MIT",
    "authors": [
        {
            "name": "Rezuan Kassim",
            "email": "rezuankassim@hotmail.com"
        }
    ],
    "homepage": "https://github.com/rezuankassim/bqanalytic",
    "keywords": ["Laravel", "BQAnalytic"],
    "require": {
        "illuminate/support": "~5|~6|~7|~8",
        "google/cloud-bigquery": "^1.15"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "RezuanKassim\\BQAnalytic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RezuanKassim\\BQAnalytic\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RezuanKassim\\BQAnalytic\\BQAnalyticServiceProvider"
            ],
            "aliases": {
                "BQAnalytic": "RezuanKassim\\BQAnalytic\\Facades\\BQAnalytic"
            }
        }
    }
}
