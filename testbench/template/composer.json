{
    "name": "laravel\/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "illuminate\/support": "~5"
    },
    "require-dev": {
        "phpunit\/phpunit": "~4.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app\/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "*"
            ]
        }
    },
    "minimum-stability": "stable"
}